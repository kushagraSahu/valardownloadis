container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python vbWeb/manage.py migrate --noinput"
    leader_only: true
  02_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python vbWeb/manage.py collectstatic --noinput"

option_settings:
  "aws:elasticbeanstalk:application:environment":
    DJANGO_SETTINGS_MODULE: "vbWeb.vbWeb.settings"
    "PYTHONPATH": "/opt/python/current/app/vbWeb:$PYTHONPATH"
  "aws:elasticbeanstalk:container:python":
    WSGIPath: vbWeb/vbWeb/wsgi.py
  "aws:elasticbeanstalk:container:python:staticfiles":
    "/static/": "www/static/"